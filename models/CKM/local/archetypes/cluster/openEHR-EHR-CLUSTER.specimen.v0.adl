				>
				["at0046"] = <
					text = <"*Processing(en)">
					description = <"*Workflow of specimen processing/handling.(en)">
				>
				["at0052"] = <
					text = <"تم حل الدم">
					description = <"تم حل الدم في العينة">
				>
				["at0053"] = <
					text = <"العينة بها فرط شحيمات">
					description = <"العينة بها فرط شحيمات">
