definition
	CLUSTER[at0000] matches {	-- Housing
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0002] occurrences matches {0..1} matches {	-- Housing situation
				name matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0017, 	-- Current housing situation
							at0018]	-- Usual housing situation
						}
					}
				}
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0004] occurrences matches {0..1} matches {	-- Description
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0003] occurrences matches {0..1} matches {	-- Tenure
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0006] occurrences matches {0..*} matches {	-- Detail
				include
					archetype_id/value matches {/.*/}
			}
			ELEMENT[at0016] occurrences matches {0..1} matches {	-- Comment
				value matches {
					DV_TEXT matches {*}
				}
			}
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Housing">
					description = <"Details about the place where an individual lives.">
				>
				["at0002"] = <
					text = <"Housing situation">
					description = <"Category of housing where the individual is living.">
					comment = <"Coding with a terminology is desirable, where possible. For example: \"no housing\", \"institution\", \"own home\". Only use the run-time name constraint if the individual is currently living in a different place from usual.">
				>
				["at0003"] = <
					text = <"Tenure">
					description = <"The type of legal right to occupy a dwelling, for example, owner or renter.">
				>
				["at0004"] = <
					text = <"Description">
					description = <"Narrative description about the housing where the individual is living.">
					comment = <"For example: \"Usually living in their own home, currently living in respite home while family is on holiday.\"">
				>
				["at0006"] = <
					text = <"Details">
					description = <"Additional details about housing.">
					comment = <"For example: social network/relationships; household; home environment.">
				>
				["at0016"] = <
					text = <"Comment">
					description = <"Additional narrative about where the individual is living not covered in other fields.">
				>
				["at0017"] = <
					text = <"Current housing situation">
					description = <"Category of housing where the individual is currently living.">
				>
				["at0018"] = <
					text = <"Usual housing situation">
					description = <"Category of housing where the individual is usually living.">
				>
			>
		>
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Bolig">
					description = <"Detaljer om stedet et individ bor.">
				>
				["at0002"] = <
					text = <"Boligsituasjon">
					description = <"Kategorisering av individets boligsituasjon.">
					comment = <"Koding med en terminologi er ønskelig, der det er mulig. For eksempel \"Ingen bolig, \"Institusjon\", \"Egen privat bolig\". Bruk run-time name constraint bare dersom individet ved registreringstidspunktet bor et annet sted enn det vanligvis bor.">
				>
				["at0003"] = <
					text = <"Eierskap">
					description = <"Individets juridiske rett til å benytte boligen, f.eks. selveier eller leier.">
					comment = <"For eksempel: Leietager eller boligeier.">
				>
				["at0004"] = <
					text = <"Beskrivelse">
					description = <"Fritekstbeskrivelse om individets boligsituasjon.">
					comment = <"For eksempel \"bor vanligvis i sitt eget hjem, men er i avlastningsbolig mens familien er på ferie\".">
				>
				["at0006"] = <
					text = <"Detaljer">
					description = <"Ytterligere detaljer om boligsituasjonen.">
					comment = <"For eksempel: sosiale nettverk eller forhold, husholdning, eller hjemmeomgivelser.">
				>
				["at0016"] = <
					text = <"Kommentar">
					description = <"Ytterligere fritekst om hvor individet bor, som ikke dekkes av andre felt.">
				>
				["at0017"] = <
					text = <"Nåværende boligsituasjon">
					description = <"Kategorisering av individets nåværende boligsituasjon.">
				>
				["at0018"] = <
					text = <"Vanlig boligsituasjon">
					description = <"Kategorisering av individets vanlige boligsituasjon.">
				>
			>
		>
	>
