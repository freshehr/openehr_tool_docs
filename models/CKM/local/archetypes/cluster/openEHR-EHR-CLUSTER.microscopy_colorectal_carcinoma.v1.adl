archetype (adl_version=1.4)
	openEHR-EHR-CLUSTER.microscopy_colorectal_carcinoma.v1

concept
	[at0000]	-- Microscopic findings - Colorectal cancer
language
	original_language = <[ISO_639-1::en]>
description
	original_author = <
		["name"] = <"Dr Ian McNicoll">
		["organisation"] = <"Ocean Informatics, UK">
		["email"] = <"ian.mcnicoll@oceaninformatics.com">
		["date"] = <"17/06/2009">
	>
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record detailed findings about colorectal cancer found on microscopic histopathological examination.">
			use = <"To record detailed findings about microscopic examination of tissue related to colorecal cancer.
Use as a component archetype in the context of a suite of archetypes that make up a histopathology report 
i.e. OBSERVATION.lab_test.histopathology and CLUSTER.macroscopy.colorectal_carcinoma.">
			keywords = <"histopathology", "cancer", "laboratory", "lab", "malignancy", "pathology", "colonic", "rectal", "anal", "GI", "gastrointestinal", "bowel">
			misuse = <"Not designed to be used within any other archetype other than OBSERVATION.lab_test.histopathology.">
			copyright = <"copyright (c) 2010 openEHR foundation">
		>
	>
	lifecycle_state = <"AuthorDraft">
	other_contributors = <"Koray Atalag, University of Auckland, New Zealand", "Matt Cordell, NEHTA, Australia", "David McKillop, NEHTA, Australia", "Cathy Richardson, NEHTA, Australia", "David Rowed, Ocean Informatics, Australia">
	other_details = <
		["references"] = <"College of American Pathologists. Protocol for the Examination of Specimens from Patients with Primary Carcinomas of the Colon and Rectum [Internet]. 2009 Mar ;[cited 2009 Jul 28 ]
Available from: http://www.cap.org/apps/docs/committees/cancer/cancer_protocols/2008/colonrectum08_pw.pdf

Royal College of Pathologists. Dataset for colorectal cancer (2nd edition) [Internet]. 2007 Dec 4;[cited 2009 Jul 29 ]
Available from: http://www.rcpath.org/index.asp?PageID=1153

Royal College of Pathologists of Australasia. Colorectal Cancer Structured Reporting Protocol (1st Edition 2010). [Internet]. 2010;[cited 2010 Mar 21 ]
Available from: http://www.rcpa.edu.au//static/File/Asset%20library/public%20documents/Publications/StructuredReporting/COLORECTAL%20CANCER%20STRUCTURED%20REPORTING%20PROTOCOL%20for%20web.pdf


">
		["MD5-CAM-1.0.1"] = <"45150B03A2A6EFB7E8B8B329C55E4A3E">
	>

definition
	CLUSTER[at0000] matches {	-- Microscopic findings - Colorectal cancer
		items cardinality matches {1..*; unordered} matches {
			CLUSTER[at0269] occurrences matches {0..1} matches {	-- Histological grading
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0270] occurrences matches {0..1} matches {	-- Grade
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0272, 	-- Well differentiated
									at0273, 	-- Moderately differentiated
									at0341, 	-- Poorly differentiated
									at0342, 	-- Undifferentiated
									at0360, 	-- Low-grade
									at0361]	-- High grade
								}
							}
						}
					}
					ELEMENT[at0271] occurrences matches {0..1} matches {	-- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
			CLUSTER[at0236] occurrences matches {0..1} matches {	-- Local invasion
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0310] occurrences matches {0..1} matches {	-- Local invasion classification
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0311, 	-- pT0
									at0312, 	-- pT1
									at0313, 	-- pT2
									at0314, 	-- pT3
									at0315, 	-- pT4a
									at0316, 	-- pT4b
									at0317, 	-- pTX
									at0343]	-- pTis
								}
							}
						}
					}
					ELEMENT[at0318] occurrences matches {0..1} matches {	-- Depth of invasion
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0319, 	-- pT3a
									at0320, 	-- pT3b
									at0321, 	-- pT3c
									at0322]	-- pT3d
								}
							}
						}
					}
					ELEMENT[at0323] occurrences matches {0..1} matches {	-- Distance of invasion
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::122]>
								list = <
									["1"] = <
										units = <"mm">
										magnitude = <|>=0.0|>
									>
								>
							>
						}
					}
				}
			}
			CLUSTER[at0244] occurrences matches {0..1} matches {	-- Surgical resection margins
				items cardinality matches {1..*; unordered} matches {
					allow_archetype CLUSTER[at0246] occurrences matches {0..*} matches {	-- Surgical resection margin
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.tumour_resection_margins\.v1/}
						exclude
							archetype_id/value matches {/.*/}
					}
				}
			}
			CLUSTER[at0252] occurrences matches {0..1} matches {	-- In-situ carcinoma findings
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0251] occurrences matches {0..1} matches {	-- In-situ carcinoma
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0253, 	-- Present
									at0254, 	-- Absent
									at0255]	-- Indeterminate
								}
							}
						}
					}
					ELEMENT[at0256] occurrences matches {0..1} matches {	-- Description
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
			CLUSTER[at0030] occurrences matches {0..1} matches {	-- Lymph node findings
				items cardinality matches {1..*; unordered} matches {
					allow_archetype CLUSTER[at0087] occurrences matches {0..1} matches {	-- Lymph node detail
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.lymph_node_metastases\.v1/}
					}
					ELEMENT[at0324] occurrences matches {0..1} matches {	-- Nodal involvement classification
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0325, 	-- pN0
									at0326, 	-- pN1
									at0327, 	-- pN2
									at0344]	-- pNx
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0276] occurrences matches {0..1} matches {	-- Distance from margin
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::122]>
								list = <
									["1"] = <
										units = <"mm">
										magnitude = <|>=0.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0348] occurrences matches {0..1} matches {	-- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
			ELEMENT[at0365] occurrences matches {0..1} matches {	-- Status of apical lymph node
				value matches {
					DV_TEXT matches {*}
				}
			}
			CLUSTER[at0278] occurrences matches {0..1} matches {	-- Local tissue invasion
				items cardinality matches {1..*; unordered} matches {
					allow_archetype CLUSTER[at0277] occurrences matches {0..*} matches {	-- Venous (large vessel) invasion
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.tumour_invasion\.v1/}
						exclude
							archetype_id/value matches {/.*/}
					}
					allow_archetype CLUSTER[at0279] occurrences matches {0..*} matches {	-- Lymphatic (small vessel) invasion
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.tumour_invasion\.v1/}
						exclude
							archetype_id/value matches {/.*/}
					}
					allow_archetype CLUSTER[at0286] occurrences matches {0..*} matches {	-- Perineural invasion
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.tumour_invasion\.v1/}
					}
					CLUSTER[at0280] occurrences matches {0..1} matches {	-- Discontinuous extramural deposit findings
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0281] occurrences matches {0..1} matches {	-- Discontinuous extramural tumour deposits
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0282, 	-- Present
											at0283, 	-- Absent
											at0284]	-- Indeterminate
										}
									}
								}
							}
							allow_archetype CLUSTER[at0346] occurrences matches {0..1} matches {	-- Discontinuous extramural deposits
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.tumour_invasion\.v1/}
								exclude
									archetype_id/value matches {/.*/}
							}
							ELEMENT[at0285] occurrences matches {0..1} matches {	-- Distance from margin
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::122]>
										list = <
											["1"] = <
												units = <"mm">
												magnitude = <|>=0.0|>
											>
										>
									>
								}
							}
						}
					}
				}
			}
			CLUSTER[at0263] occurrences matches {0..1} matches {	-- Distant metastasis findings
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0264] occurrences matches {0..1} matches {	-- Distant metastasis
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0362, 	-- Present
									at0363, 	-- Absent
									at0364]	-- Indeterminate
								}
							}
						}
					}
					allow_archetype CLUSTER[at0287] occurrences matches {0..*} matches {	-- Sites 
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location-precise\.v1/}
						exclude
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0265] occurrences matches {0..1} matches {	-- Description
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
			CLUSTER[at0366] occurrences matches {0..1} matches {	-- Adenomatous polyps
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0359] occurrences matches {0..1} matches {	-- Type of polyp
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0338] occurrences matches {0..1} matches {	-- Number of polyps
						value matches {
							DV_COUNT matches {
								magnitude matches {|>=0|}
							}
						}
					}
					ELEMENT[at0367] occurrences matches {0..1} matches {	-- Polyposis syndrome
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0368, 	-- Present
									at0369, 	-- Absent
									at0370]	-- Indeterminate
								}
							}
						}
					}
					ELEMENT[at0371] occurrences matches {0..1} matches {	-- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
			CLUSTER[at0179] occurrences matches {0..1} matches {	-- Additional findings
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0250] occurrences matches {0..*} matches {	-- Additional finding
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0293, 	-- Synchronous carcinoma
									at0294, 	-- Ulcerative colitis
									at0295, 	-- Ulcerative colitis with dysplasia
									at0296, 	-- Crohn's disease
									at0297, 	-- Crohn's disease with dysplasia
									at0340]	-- Diverticulosis
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0220] occurrences matches {0..1} matches {	-- Description
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
			ELEMENT[at0262] occurrences matches {0..1} matches {	-- Residual tumour status
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0298, 	-- R0
							at0299, 	-- R1
							at0300, 	-- R2
							at0301]	-- RX
						}
					}
				}
			}
			CLUSTER[at0308] occurrences matches {0..1} matches {	-- Response to neoadjuvant therapy
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0330] occurrences matches {0..1} matches {	-- Neoadjuvant therapy given
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0331, 	-- Yes
									at0332, 	-- No
									at0333]	-- Not known
								}
							}
						}
					}
					CLUSTER[at0347] occurrences matches {0..1} matches {	-- Grade of response
						items cardinality matches {1; unordered} matches {
							ELEMENT[at0302] occurrences matches {0..1} matches {	-- Grade (AJCC)
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0349, 	-- Grade 0 - complete response
											at0350, 	-- Grade 1 - moderate response
											at0351, 	-- Grade 2 - minimal response
											at0352]	-- Grade 3 - poor response
										}
									}
								}
							}
							ELEMENT[at0329] occurrences matches {0..1} matches {	-- Grade (CAP)
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0354, 	-- No residual tumour
											at0355, 	-- Marked response
											at0356, 	-- Moderate response
											at0357, 	-- No definite response
											at0358]	-- Response not known
										}
									}
								}
							}
							ELEMENT[at0334] occurrences matches {0..1} matches {	-- Grade (RCP)
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0335, 	-- No residual tumour cells or mucous lakes only
											at0336, 	-- Minimal residual tumour
											at0337]	-- No marked regression
										}
									}
								}
							}
						}
					}
					ELEMENT[at0309] occurrences matches {0..1} matches {	-- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}


