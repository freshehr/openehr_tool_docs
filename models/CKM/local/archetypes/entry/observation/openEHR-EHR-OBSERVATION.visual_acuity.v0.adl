				["at0025"] = <
					text = <"Measurement Device Details">
					description = <"Details of the device used to measure visual acuity.">
				>
				["at0028"] = <
					text = <"Notation">
					description = <"Details of a visual acuity result recorded using one of the result notation formats.">
