					text = <"Tree">
					description = <"@ internal @">
				>
				["at0021"] = <
					text = <"Chart Optotype">
					description = <"The style of chart optotype used to assess visual acuity.">
				>
				["at0022"] = <
					text = <"Chart Scoring Algorithm">
