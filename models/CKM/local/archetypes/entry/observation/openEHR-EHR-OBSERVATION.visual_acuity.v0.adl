	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Visual acuity test result">
					description = <"Visual acuity is a measure of the spatial resolution of the visual processing system.">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0003"] = <
					text = <"Tree">
