																		precision = <|0|>
																	>
																	["2"] = <
																		units = <"kHz">
																		magnitude = <|0.125..16.0|>
																		precision = <|3|>
																	>
																>
																assumed_value = <
																	magnitude = <0.125>
																	units = <"kHz">
																	precision = <3>
																>
															>
														}
													}
													ELEMENT[at0012] matches {	-- Threshold Level
