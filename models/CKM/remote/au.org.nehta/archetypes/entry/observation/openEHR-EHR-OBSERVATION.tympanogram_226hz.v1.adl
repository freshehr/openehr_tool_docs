												}
											}
											ELEMENT[at0004] occurrences matches {0..1} matches {	-- Tympanogram Type
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0005, 	-- Type A
															at0006, 	-- Type B
															at0057, 	-- Type B with high ECV
															at0007, 	-- Type C
															at0070, 	-- Type D
															at0008, 	-- Type As
															at0034, 	-- Type Ad
															at0035]	-- Indeterminate
														}
													}
												}
											}
											ELEMENT[at0071] occurrences matches {0..1} matches {	-- Tympanogram Type B Subtype
												value matches {
