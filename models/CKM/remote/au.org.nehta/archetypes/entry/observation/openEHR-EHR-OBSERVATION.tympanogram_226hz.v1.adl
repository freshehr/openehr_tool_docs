													}
												}
											}
											ELEMENT[at0031] occurrences matches {0..1} matches {	-- No Test Result
												value matches {
													DV_BOOLEAN matches {
														value matches {True}
													}
												}
											}
											ELEMENT[at0032] occurrences matches {0..1} matches {	-- Reason for No Test Result
												value matches {
