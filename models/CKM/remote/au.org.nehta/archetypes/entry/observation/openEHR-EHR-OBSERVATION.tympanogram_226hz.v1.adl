														defining_code matches {
															[local::
															at0059, 	-- Type C1
															at0060, 	-- Type C2
															at0061]	-- Type C3
														}
													}
												}
											}
											ELEMENT[at0033] occurrences matches {0..1} matches {	-- Clinical Interpretation
												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
									ELEMENT[at0036] occurrences matches {0..1} matches {	-- Overall Interpretation
										value matches {
